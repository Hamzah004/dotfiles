{
  "layer": "top",
  "height": 26,
  "spacing": 1,
  "margin-top" :2,
  "margin-right" :4,
  "margin-left" :4,

  "modules-left": [
    // "custom/rofi",
    "sway/workspaces",
    "sway/window"
  ],

  "modules-center": ["clock","custom/timer",],

  "modules-right": [
    // "custom/color-picker",
    // "disk",
    "sway/language",
    "cpu",
    "temperature",
    "backlight",
    "custom/memory",
    "pulseaudio",
    "pulseaudio#source",
    "battery",
    "power-profiles-daemon",
    "custom/swaync",
    "tray"
  ],

  "custom/rofi": {
		"format": "<span >Óò™ </span>",
		"on-click": "rofi -show drun -disable-history -show-icons",
    "on-click-right": "$HOME/.config/custom_scripts/power.sh",
    "escape" : true,
    "tooltip" : false
	},

  // Modules configuration
  "sway/workspaces": {
    "disable-scroll": true,
    "active-only": false,
    "all-outputs": true,
    "warp-on-scroll": false,
    "format": "{icon}",
    "format-icons": {
      "1": "<span >Û∞ß®</span>",
      "2": "<span >Û∞àπ</span>",
      "3": "<span >ÔÑì</span>",
      "4": "<span >ÔÅª</span>",
      "5": "<span >ÔÉ†</span>",
      "6": "<span >Û∞≠õ</span>",
      "7": "<span >Û±ì∑</span>",
      "8": "<span >Û∞öÄ</span>"
    }
  },

  "sway/window": {
    "format": "<span color='#1e1e2e' bgcolor='#d3869b' > Û∞£Ü </span> {app_id}",
    "separate-outputs": true,
    "icon":false,
  },

  "hyprland/workspaces": {
    "disable-scroll": true,
    "active-only": false,
    "all-outputs": true,
    "warp-on-scroll": false,
    "format": "{icon}",
    "format-icons": {
      "1": "<span >Û∞ß®</span>",
      "2": "<span >Û∞àπ</span>",
      "3": "<span >ÔÑì</span>",
      "4": "<span >ÔÅª</span>",
      "5": "<span >ÔÉ†</span>",
      "6": "<span >Û∞≠õ</span>",
      "7": "<span >Û±ì∑</span>",
      "8": "<span >Û∞öÄ</span>"
    }
  },

  "hyprland/window": {
    "format": "<span color='#1e1e2e' bgcolor='#d3869b' > Û∞£Ü </span> {class}",
    "separate-outputs": true,
    "icon":false,
  },
    "tray": {
        "icon-size": 13,
        "spacing": 10
    },

  "disk": {
    "format": "<span color='#1e1e2e' bgcolor='#ea6962' > Ôà≥ </span> {free}",
    "interval": 20
  },

  "cpu": {
    "format": "<span color='#1e1e2e' bgcolor='#cba6f7' > Ôãõ {usage}% </span>",
    "tooltip": false,
    "interval": 1
  },

  "temperature": {
    "tooltip":false,
    "thermal-zone": 2,
    "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "critical-threshold": 70,
    "format": "<span color='#1e1e2e' bgcolor='#f5c2e7' > Ôãâ {temperatureC}¬∞C </span>",
    "format-critical": "<span color='#1e1e2e' bgcolor='#cc241d' > Ó∏ï {temperatureC}¬∞C</span>",
  },

  "backlight": {
    // "device": "acpi_video1",
    "format": "<span color='#1e1e2e' bgcolor='#F9A825' > {icon} {percent}% </span>",
    "format-icons": ["üåë", "üåò", "üåó", "üåñ", "üåï"],
    "tooltip": false
  },

  "custom/memory": {
    "exec": "$HOME/.config/custom_scripts/memory_usage.sh",
    "interval": 2,
    "return-type": "json",
    "format": "<span color='#1e1e2e' bgcolor='#f38ba8'> ÓøÖ  {} </span>"
  },

 "pulseaudio": {
    // "scroll-step": 1, // %, can be a float
    "format": "<span color='#1e1e2e' bgcolor='#89B4FA' > ÔÄ®  {volume}% </span>",
    "format-muted": "<span color='#1e1e2e' bgcolor='#f38ba8' > Óª®  {volume}% </span>",
    "format-bluetooth": "<span color='#1e1e2e' bgcolor='#89B4FA' > Û∞Ç∞ {volume}% </span>",
    "format-bluetooth-muted": "<span color='#1e1e2e' bgcolor='#f38ba8' > Û∞Ç≤ {volume}% </span>",
    "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
    "tooltip": false,
    "max-volume": 100
  },

    "pulseaudio#source": {
        "format": "{format_source}",
        "format-source": "<span color='#1e1e2e' bgcolor='#89B4FA' > ÔÑ∞ {volume}% </span>",
        "format-source-muted": "<span color='#1e1e2e' bgcolor='#f38ba8' > ÔÑ±  {volume}% </span>",
        "on-click": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",
        "on-click-right": "/usr/bin/pavucontrol",
        "on-scroll-down": "pactl set-source-volume @DEFAULT_SOURCE@ -1%",
        "on-scroll-up": "pactl set-source-volume @DEFAULT_SOURCE@ +1%",
        "tooltip": false,
    },

  "battery#bat2": {
    "bat": "BAT2"
  },

  "battery": {
    "interval": 1,
    "states": {
      "good": 99,
      "warning": 30,
      "critical": 20
    },
    "format-icons": ["Û∞Çé","Û∞Å∫","Û∞Åª","Û∞ÅΩ","Û∞Åæ","Û∞Åø","Û∞ÇÄ","Û∞ÇÅ","Û∞ÇÇ","Û∞Åπ"],
    "format": "<span color='#1e1e2e' bgcolor='#a6da95' > {icon} {capacity}% </span>",
    "format-critical": "<span color='#1e1e2e' bgcolor='#cc241d' > {icon} {capacity}% !! </span>",
    "format-warning": "<span color='#1e1e2e' bgcolor='#e78a4e' > {icon} {capacity}% </span>",
    "format-full": "<span color='#1e1e2e' bgcolor='#a6da95' > {icon} {capacity}% </span>",
    "format-charging": "<span color='#1e1e2e' bgcolor='#a6da95' > Û∞ÇÖ {capacity}% </span>",
    "format-charging-warning": "<span color='#1e1e2e' bgcolor='#e78a4e' > Û∞¢ù {capacity}% </span>",
    "format-charging-critical": "<span color='#1e1e2e' bgcolor='#cc241d' > Û∞¢ú {capacity}% </span>",
    "format-plugged": "<span color='#1e1e2e' bgcolor='#a6da95' > Û∞ÇÖ {capacity}% </span>",
    "format-alt": "<span color='#1e1e2e' bgcolor='#a6da95' > Û±ß• {time} </span>",
    "tooltip": false
  },

"clock": {
 "format": "<span color='#1e1e2e' bgcolor='#a6da95' > ÔÅ≥ {:%H:%M} </span>",
        "interval":1,
        "format-alt": "<span color='#1e1e2e' bgcolor='#a6da95' > ÔÅ≥ {:%a,%d %b,%Y(%H:%M:%S)} </span>",
        "tooltip-format": "\n<span size='12pt' font='MesloLGMDZ Nerd Font'>{calendar}</span>",
        "calendar": {
                    "mode"          : "year",
                    "mode-mon-col"  : 3,
                    "weeks-pos"     : "right",
                    "on-scroll"     : 1,
                    "format": {
                              "months":     "<span color='#ffead3'><b>{}</b></span>",
                              "days":       "<span color='#ecc6d9'><b>{}</b></span>",
                              "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
                              "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
                              "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
                              }
                    },
        "actions":  {
                    "on-click-right": "mode",
                    "on-click-forward": "tz_up",
                    "on-click-backward": "tz_down",
                    "on-scroll-up": "shift_up",
                    "on-scroll-down": "shift_down"
                    }
    },

"power-profiles-daemon": {
  "format-icons": {
    "performance": "ÔÅ≠",
    "balanced": "Ôâé ",
    "power-saver": "ÔÅ¨ "
  },
  "format": "<span color='#1e1e2e' bgcolor='#a6da95'> {icon} </span>",
  "tooltip-format": "Power Mode: {profile}",
},

"custom/timer": {
    "tooltip": false,
    "min-length": 5,
    "exec": "$HOME/.config/custom_scripts/timer.sh",
    "on-click": "date --date='30 minutes' +%s > /var/tmp/waybar_timer",
    "on-click-right": "echo READY > /var/tmp/waybar_timer",
    "on-click-middle": "echo READY > /var/tmp/waybar_timer",
    "on-scroll-up": "date --date='5 minute' +%s > /var/tmp/waybar_timer",
    "on-scroll-down": "date --date='1 minute' +%s > /var/tmp/waybar_timer",
    "interval": 1
},

"sway/language": {
  "format": "<span color='#1e1e2e' bgcolor='#eba0ac'> {short} </span>",
    "on-click": "swaymsg input type:keyboard xkb_switch_layout next",
    "tooltip": false,
},
    "custom/swaync": {
        "escape": true,
        "exec": "swaync-client -swb",
        "exec-if": "swaync-client",
        "format": " {icon}",
        "format-icons": {
            "dnd-inhibited-none": "Ôá∂ ",
            "dnd-inhibited-notification": "Ôá∂ ",
            "dnd-none": "Ôá∂ ",
            "dnd-notification": "Ôá∂ ",
            "inhibited-none": "ÔÉ≥ ",
            "inhibited-notification": "ÔÉ≥ ",
            "none": "ÔÉ≥ ",
            "notification": "ÔÉ≥ "
        },
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "return-type": "json",
        "tooltip": false
    },
  "custom/color-picker" : {
    "format": "<span color='#1e1e2e' bgcolor='#F9E2AF'> Û∞Éâ </span>",
    "on-click": "$HOME/bin/wl-color-picker clipboard",
    "tooltip": false
  }
}
